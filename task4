#include<iostream>
using namespace std;

main()
{
        int n,sum=0;
        int arr[n];
        cout<<"enter the number of elements in an array\n";
        cin>>n;
        cout<<"enter the elements \n";
        for(int i=0;i<n;i++)
        {
                cin>>arr[i];
                sum=sum+arr[i];
        }

        cout<<"sum is "<<sum<<"\n";
}
