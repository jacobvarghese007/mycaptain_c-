#include<iostream>
using namespace std;

main()
{
        int n,year,week,day;
        cout<<"enter the number of days \n";
        cin>>n;

        year=n/365;

        week=(n-(year*365))/7;

        day=n-((year*365)+(week*7));

        cout<<year<<" years\t"<<week<<" weeks\t"<<day<<" days\n";
}
