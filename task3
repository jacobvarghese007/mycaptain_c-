#include<iostream>
using namespace std;

main()
{
        int n;
        cout<<"enter the year \n";
        cin>>n;

        if(n%100==0)
        {
        if(n%400==0)
        {
        cout<<"leap year\n";
        
        }
        else
        cout<<"not a leap year\n";
        
        }
        else
        {

        if(n%4==0)
        {
                cout<<n<<" is leap year \n";
        }
        else
                cout<<n<<" is not a leap year \n";
                }
}
